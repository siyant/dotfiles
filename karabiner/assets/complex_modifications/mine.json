{
  "title": "siyan's",
  "rules": [
    {
      "description": "Simultaneous Equal+Delete to forward delete",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "equal_sign"
              },
              {
                "key_code": "delete_or_backspace"
              }
            ],
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "delete_forward"
            }
          ]
        }
      ]
    },
    {
      "description": "CAPS LOCK + Firefox/Terminal/Vscode/Sublime/Macpass/A(finder)/Chrome/Windows/Postman/Intellij/toDoist",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "shell_command": "open /Applications/Firefox.app"
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "caps_lock pressed",
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "t",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "shell_command": "open /Applications/Utilities/Terminal.app"
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "caps_lock pressed",
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Sublime Text.app'"
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
