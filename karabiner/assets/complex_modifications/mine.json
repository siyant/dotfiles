    },
    {
      "description": "Control + J/K for switching tabs",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": ["control"]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": ["control", "shift"]
            }
          ]
        }
      ]
    }
  ]
}
